{
    "name": "example/minio-app",
    "description": "A PHP application that uses MinIO via AWS SDK",
    "require": {
        "aws/aws-sdk-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
